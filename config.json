    "formats": {
        "proxyJoin": {
            "color": "#F758FF",
            "extra": [
                {
                    "color": "#50C5FF",
                    "text": "joined $CURRENT_SERVER_ALIAS"
                }
            ],
            "text": "$PLAYER_NAME "
        },
        "proxyLeft": {
            "color": "#F758FF",
            "extra": [
                {
                    "color": "50C5FF",
                    "text": "left $CURRENT_SERVER_ALIAS"
                }
            ],
            "text": "$PLAYER_NAME "
        },
        "serverJoin": {
            "color": "#F758FF",
            "extra": [
                {
                    "color": "50C5FF",
                    "text": "left $PREVIOUS_SERVER_ALIAS, joined $CURRENT_SERVER_ALIAS"
                }
            ],
            "text": "$PLAYER_NAME "
        },
        "foreignChat": {
            "extra": [
                {
                    "clickEvent": {
                        "action": "suggest_command",
                        "value": "/msg $PLAYER_NAME "
                    },
                    "hoverEvent": {
                        "action": "show_text",
                        "contents": {
                            "extra": [
                                {
                                    "italic": true,
                                    "color": "dark_green",
                                    "text": "minecraft:player\n"
                                },
                                "UUID: ",
                                {
                                    "italic": true,
                                    "color": "dark_green",
                                    "text": "$PLAYER_UUID"
                                }
                            ],
                            "text": "Type: "
                        }
                    },
                    "text": "$PLAYER_NAME"
                },
                "\u003e $MESSAGE"
            ],
            "text": "left $CURRENT_SERVER_ALIAS \u003c"
        },
        "serverList": {
            "title": "Welcome $PLAYE_NAME to server! Server list: \n",
            "serverItem": "$CURRENT_SERVER_ALIAS: $PLAYER_LIST\n",
            "playerItem": {
                "color": "white",
                "clickEvent": {
                    "action": "suggest_command",
                    "value": "/msg $PLAYER_NAME "
                },
                "hoverEvent": {
                    "action": "show_text",
                    "contents": {
                        "extra": [
                            {
                                "italic": true,
                                "color": "gray",
                                "text": "$PLAYER_PING ms"
                            }
                        ],
                        "text": "Ping: "
                    }
                },
                "text": "$PLAYER_NAME "
            },
            "emptyPlayerList": {
                "italic": true,
                "color": "dark_gray",
                "text": "(No Players)"
            }
        }
    },
    "sendHellomessage": true
}